def turn_right():
    turn_left()
    turn_left()
    turn_left()

def jump_hurdle():
    turn_left()
    while wall_on_right() == True:
        move()
    turn_right()
    move()
    turn_right()
    move()
    while wall_in_front() == False:
        move()
    turn_left()

while not at_goal():
    if wall_in_front() == False:
        move()
    elif wall_in_front() == True: 
        jump_hurdle()
